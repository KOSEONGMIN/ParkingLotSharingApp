<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/join2"
    tools:context=".Join2Activity">

    <android.support.constraint.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="40dp" />

    <TextView
        android:id="@+id/carNumberText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="145dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/jalnanfont"
        android:text="@string/carnumber"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.205" />

    <TextView
        android:id="@+id/carTypeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="73dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/jalnanfont"
        android:text="@string/cartype"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.089"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carNumberText"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/preButton"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="48dp"
        android:background="@drawable/arrow_left"
        android:text="    "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/confirmButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="48dp"
        android:background="#00000000"
        android:fontFamily="@font/jalnanfont"
        android:text="가입완료"
        android:textColor="@color/white"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.892"
        app:layout_constraintStart_toEndOf="@+id/preButton" />
    <EditText
        android:id="@+id/carNumberInput1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:ems="2"
        android:fontFamily="@font/jalnanfont"
        android:gravity="center"
        android:inputType="number"
        android:maxLength="2"
        android:background="@null"
        android:textSize="20dp"
        android:textColor="@color/white"
        app:layout_constraintBaseline_toBaselineOf="@+id/carNumberText"
        app:layout_constraintEnd_toStartOf="@+id/carNumberInput2" />

    <EditText
        android:id="@+id/carNumberInput2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:ems="2"
        android:fontFamily="@font/jalnanfont"
        android:gravity="center"
        android:background="@null"
        android:inputType="textPersonName"
        android:maxLength="2"
        android:nextFocusDown="@id/carNumberInput3"
        android:textSize="20dp"
        android:textColor="@color/white"
        app:layout_constraintBaseline_toBaselineOf="@+id/carNumberInput1"
        app:layout_constraintEnd_toStartOf="@+id/carNumberInput3" />

    <EditText
        android:id="@+id/carNumberInput3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:ems="4"
        android:fontFamily="@font/jalnanfont"
        android:gravity="center"
        android:inputType="number"
        android:maxLength="4"
        android:background="@null"
        android:textSize="20dp"
        android:textColor="@color/white"
        app:layout_constraintBaseline_toBaselineOf="@+id/carNumberInput2"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/carTypeInput"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/jalnanfont"
        android:gravity="center"
        android:spinnerMode="dialog"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.915"
        app:layout_constraintStart_toEndOf="@+id/carTypeText"
        app:layout_constraintTop_toBottomOf="@+id/carNumberText" />

</android.support.constraint.ConstraintLayout>